{
    "manifest_version": 3,
    "name": "Minimal New Tab",
    "version": "1.0",
    "description": "A custom new tab page to improve productivity",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": [
      "storage"
    ],
    "icons": {
      "16": "images/new-tab.png",
      "48": "images/new-tab.png",
      "128": "images/new-tab.png"
    }
  }
  